{
  "id": null,
  "title": "Monitoring Simple",
  "tags": ["monitoring"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Usage CPU",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "CPU %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}
