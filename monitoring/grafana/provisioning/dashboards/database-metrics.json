{
  "dashboard": {
    "id": null,
    "title": "Base de Données - MySQL & Redis",
    "tags": ["database", "mysql", "redis"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Connexions MySQL",
        "type": "timeseries",
        "targets": [
          {
            "expr": "mysql_global_status_threads_connected",
            "legendFormat": "Connexions actives"
          },
          {
            "expr": "mysql_global_variables_max_connections",
            "legendFormat": "Connexions max"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Requêtes MySQL par Seconde",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(mysql_global_status_queries[5m])",
            "legendFormat": "Requêtes/sec"
          },
          {
            "expr": "rate(mysql_global_status_com_select[5m])",
            "legendFormat": "SELECT/sec"
          },
          {
            "expr": "rate(mysql_global_status_com_insert[5m])",
            "legendFormat": "INSERT/sec"
          },
          {
            "expr": "rate(mysql_global_status_com_update[5m])",
            "legendFormat": "UPDATE/sec"
          },
          {
            "expr": "rate(mysql_global_status_com_delete[5m])",
            "legendFormat": "DELETE/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Temps de Réponse MySQL",
        "type": "timeseries",
        "targets": [
          {
            "expr": "mysql_global_status_slow_queries",
            "legendFormat": "Requêtes lentes"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Buffer Pool MySQL",
        "type": "timeseries",
        "targets": [
          {
            "expr": "mysql_global_status_innodb_buffer_pool_pages_total",
            "legendFormat": "Pages totales"
          },
          {
            "expr": "mysql_global_status_innodb_buffer_pool_pages_free",
            "legendFormat": "Pages libres"
          },
          {
            "expr": "mysql_global_status_innodb_buffer_pool_pages_dirty",
            "legendFormat": "Pages modifiées"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "Taille Base de Données",
        "type": "stat",
        "targets": [
          {
            "expr": "mysql_info_schema_table_size_data_length{schema=\"hospital_db\"}",
            "legendFormat": "Données"
          },
          {
            "expr": "mysql_info_schema_table_size_index_length{schema=\"hospital_db\"}",
            "legendFormat": "Index"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 6,
        "title": "Redis - Clients Connectés",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Clients connectés"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Redis - Utilisation Mémoire",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Mémoire utilisée"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Mémoire max"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 8,
        "title": "Redis - Opérations par Seconde",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commandes/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 9,
        "title": "Redis - Hit Rate Cache",
        "type": "gauge",
        "targets": [
          {
            "expr": "redis_keyspace_hits / (redis_keyspace_hits + redis_keyspace_misses) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Tables les Plus Utilisées",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, mysql_info_schema_table_rows{schema=\"hospital_db\"})",
            "legendFormat": "{{table}}"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
